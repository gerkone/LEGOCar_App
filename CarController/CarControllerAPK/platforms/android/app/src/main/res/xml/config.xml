<?xml version='1.0' encoding='utf-8'?>
<widget id="it.tessaro.CarController" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CDVOrientation">
        <param name="android-package" value="cordova.plugins.screenorientation.CDVOrientation" />
    </feature>
    <feature name="WifiWizard">
        <param name="android-package" value="com.pylonproducts.wifiwizard.WifiWizard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Permissions">
        <param name="android-package" value="com.android.plugins.Permissions" />
    </feature>
    <feature name="networkinterface">
        <param name="android-package" value="com.albahra.plugin.networkinterface.networkinterface" />
    </feature>
    <feature name="DHCPInfo">
        <param name="android-package" value="org.apache.cordova.android.DHCP" />
    </feature>
    <feature name="WifiManagerPlugin">
        <param name="android-package" value="dk.kapetanovic.wifimanager.WifiManagerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>CarController</name>
    <description>
        A controller app for the lego record breaking car.
    </description>
    <author email="fabio.tessaro.porta@gmail.com" href="https://plus.google.com/u/0/101140664454077604787">
        Fabio Tessaro
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="Fullscreen" value="true" />
</widget>
